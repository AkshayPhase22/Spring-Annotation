<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<context:annotation-config/>

<bean class="com.demo.Spring.Student" name="student">
<property name="stdId" value="1919"></property>
<property name="stdName" value="Akshay"></property>
</bean>

<bean class="com.demo.Spring.Address" name="address22">
<property name="city" value="Satara"></property>
</bean>


<bean class="com.demo.Spring.Address" name="address2">
<property name="city" value="pune"></property>
</bean>




</beans>